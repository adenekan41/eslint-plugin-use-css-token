{
 "name": "eslint-plugin-use-css-token",
 "version": "1.0.0",
 "description": "Utilize CSS tokenization for color values instead of static, pre-defined color codes in CSS/SCSS code.",
 "main": "dist/index.cjs.js",
 "module": "dist/index.es.js",
 "files": ["dist"],
 "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
 "repository": "git://github.com/adenekan41/eslint-plugin-use-css-token.git",
 "scripts": {
  "build": "vite build",
  "dev": "vite build --watch",
  "lint": "eslint . --ext .js,.ts,.tsx"
 },
 "keywords": [
  "eslint",
  "plugin"
 ],
 "homepage": "https://github.com/adenekan41/eslint-plugin-use-css-token#readme",
 "author": "Adenekan Wonderful <hellocodewonders@gmail.com> (https://codewonders.dev)",
 "license": "MIT",
 "dependencies": {
  "postcss": "^8.3.6"
 },
 "devDependencies": {
  "@babel/core": "^7.16.0",
  "@babel/eslint-parser": "^7.16.0",
  "@types/node": "^18.15.11",
  "eslint": "^8.1.0",
  "vite": "^4.2.1"
 }
}
